#include <iostream>
#include <bits/stdc++.h>
#include <string>
using namespace std;

int mlap[500][500],vis[500][500],ans,xc[4]= {-1,1,0,0},yc[4]= {0,0,-1,1};

int x,y;
struct secter
{
  int h,l;
} slet[250000];
int strToint(string a){
  int result = 0;
  for(int i=0;i<a.size();i++){
    result = result*10 + a[i]-'0';
  }
  return result;
}
//Compiler version g++ 6.3.0
void bfs(int x,int y)
{
 
  int head=1,tail=1;
  mlap[x][y]=2;
  vis[x][y]=1;
  slet[tail].h=x;
  slet[tail].l=y;
  tail++;
  while(head<tail)
  {
    x=slet[head].h;
    y=slet[head].l;
    head++;
    for(int i=0; i<4; i++)
    {
      if((!vis[x+xc[i]][y+yc[i]]))
      {
        slet[tail].h=x+xc[i];
        slet[tail].l=y+yc[i];
        mlap[x+xc[i]][y+yc[i]]=2;
        vis[x+xc[i]][y+yc[i]]=1;
        tail++;
      }
    }
  }
}
int main()
{
    string str,str_x,str_y;
    getline(cin,str);
    int check =0;
    for(int i=0;i<str.size();i++){
      if(str[i]!=' '&&check!=1){
        str_x +=str[i];
      }else if(str[i]==' '){
        check=1;
      }else if(str[i]!=' '&&check==1){
        str_y +=str[i];
      }
    }
    
    y=strToint(str_y);
    x=strToint(str_x);
    
    for(int t=1;t<=y;t++){
      string str;
      cin>>str;
      for(int s=1;s<=str.length();s++){
        if(str[s-1]=='0'){
          mlap[s+1][t+1]=0;
          }else{
          mlap[s+1][t+1]=1;
          }
        if(mlap[s+1][t+1]==1)
        vis[s+1][t+1]=1;
      }
      }
      for(int hk=0; hk<y+1;hk++){
        for(int kh=0;kh<x+1;kh++){
          if(mlap[kh][hk]!=1){
            mlap[kh][hk]=0;
            }
          }
      }
      for(int i=0; i<=(x+1); i++)
    for(int j=0; j<=(y+1); j++)
    {
      if(!i||i==(x+1))  mlap[i][j]=1,vis[i][j]=1;
      if(!j||j==(y+1))  mlap[i][j]=1,vis[i][j]=1;
    }
  bfs(1,1);
  for(int i=1; i<=x; i++)
  {
    for(int j=1; j<=y; j++)
    {
      if(mlap[i][j]==0)  ans++;
    }
  }
  cout<<ans<<endl;
  return 0;
      
    
}
